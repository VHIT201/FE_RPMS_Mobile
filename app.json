{
  "expo": {
    "name": "quanlycanho",
    "slug": "quanlycanho",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#ffffff",
      "resize": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vanhoangi2001.quanlycanho"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vanhoangi2001.quanlycanho"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/splash-icon.png",
          "dark": {
            "image": "./assets/splash-icon.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 100
        }
      ],
      [
        "onesignal-expo-plugin",
        {
          "mode": "development" 
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "08d852de-8290-4be1-b0e8-a1cefdfdf620",
        "oneSignalAppId": "bfb77e57-16be-400a-9936-34248d4b9607"
      }
    }
  }
}
